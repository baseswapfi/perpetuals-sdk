    "@ethersproject/solidity": "^5.0.9",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "swr": "^1.3.0",
    "viem": "1.1.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/lodash": "^4.14.178",
    "date-fns": "^2.21.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
